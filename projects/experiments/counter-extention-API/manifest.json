{
  "name":"counter",
  "version":"1.0",
  "description": "My first extention which will replace me",
  "manifest_version": 2,
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["background.js"]
    }
  ],
  "browser_action":{
    "default_icon":"tent.png",
    "default_popup":"popup/index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
